<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Account"
            android:parentActivityName=".Settings"/>
        <activity android:name=".About"
            android:parentActivityName=".Settings"/>
        <activity android:name=".Filters"
            android:parentActivityName=".Settings"/>
        <activity android:name=".AlertSettings"
            android:parentActivityName=".Settings"/>
        <activity android:name=".Privacy"
            android:parentActivityName=".Settings"/>
        <activity android:name=".PopUpBlocking"
            android:parentActivityName=".Settings"/>
        <activity android:name=".Passwords"
            android:parentActivityName=".Settings"/>
        <activity android:name=".Favourites"
            android:parentActivityName=".Settings"/>
        <activity android:name=".Background"
            android:parentActivityName=".Settings"/>
        <activity android:name=".Shortcuts"
            android:parentActivityName=".Settings"/>
        <activity android:name=".TimeTable"
            android:parentActivityName=".Settings"/>
        <activity android:name=".HomePageWidgets"
            android:parentActivityName=".Settings"/>
        <activity android:name=".HomePage"/>
        <activity android:name=".ScreenTime"
            android:parentActivityName=".Settings"/>
        <activity android:name=".BlackList"
            android:parentActivityName=".Settings"/>
        <activity android:name=".Settings"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>